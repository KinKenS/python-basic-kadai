        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"名前は{self.name}、年齢は{self.age}才\")\n",
        "\n",
        "human = Human(\"侍太郎\", 30)\n",
        "human.printinfo()"
